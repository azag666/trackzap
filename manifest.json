{
  "manifest_version": 3,
  "name": "WhatsApp Sales Pro",
  "version": "1.0",
  "permissions": ["storage", "activeTab"],
  "host_permissions": ["https://web.whatsapp.com/*", "https://*.vercel.app/*"],
  "content_scripts": [
    {
      "matches": ["https://web.whatsapp.com/*"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}
